<div class="container m-5">
  <div class="todo-title mb-3">
    <h1>My Todo List</h1>
  </div>
  {{#each todos}}
  <div class="form-check my-1">
    <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
    <label class="form-check-label" for="flexCheckDefault">
      {{ this.name }}
    </label>
  </div>
  {{/each}}
</div>